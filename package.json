    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "jsr": "^0.12.4",
